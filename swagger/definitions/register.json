{
  "/register": {
    "post": {
      "tags": [
        "Authentication"
      ],
      "summary": "Register",
      "produces": [
        "application/xml",
        "application/json"
      ],
      "parameters": [
        {
          "in": "body",
          "name": "username",
          "type": "string",
          "format": "string",
          "required": true
        },
        {
          "in": "body",
          "name": "email",
          "type": "string",
          "format": "email",
          "required": true
        },
        {
          "in": "body",
          "name": "password",
          "type": "string",
          "format": "password",
          "minLength": 8,
          "required": true
        }
      ],
      "responses": {}
    }
  }
}
